{
  "name": "express-reference-server",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "jest": "jest --runInBand",
    "test": "npm run jest -- --coverage",
    "test:watch": "npm run jest -- --watchAll",
